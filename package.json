{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript Nodejs Api",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "test": "yarn test:unit && yarn test:functional",
    "lint": "eslint ./src ./tests --ext .ts",
    "lint:fix": "eslint ./src ./tests --ext .ts --fix",
    "style:check": "prettier --check src/**/*.ts tests/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts tests/**/*.ts",
    "test:functional": "jest --projects ./tests --runInBand",
    "test:unit": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasdrta/node-typescript-api.git"
  },
  "keywords": [
    "nodejs",
    "api",
    "typescript"
  ],
  "author": "Lucas Duarte",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lucasdrta/Beach-Forecast-API/issues"
  },
  "homepage": "https://github.com/lucasdrta/Beach-Forecast-API#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "^0.0.36",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.9",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "^5.7.36",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.0.27",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "jest": "^26.2.2",
    "nock": "^13.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.4",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.9.28"
  }
}
