{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript Nodejs Api",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "lint": "eslint ./src ./tests --ext .ts",
    "lint:fix": "eslint ./src ./tests --ext .ts --fix",
    "test:functional": "jest --projects ./tests --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasdrta/node-typescript-api.git"
  },
  "keywords": [
    "nodejs",
    "api",
    "typescript"
  ],
  "author": "Lucas Duarte",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lucasdrta/node-typescript-api/issues"
  },
  "homepage": "https://github.com/lucasdrta/node-typescript-api#readme",
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.0.27",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "jest": "^26.2.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "module-alias": "^2.2.2"
  }
